# Example backend environment variables
# Copy this file to backend/.env and fill in real values. Do NOT commit real secrets.

# MongoDB connection string (example for local)
MONGODB_URI=mongodb://localhost:27017/studentdb

# Supabase project URL (public)
SUPABASE_URL=https://<your-project-ref>.supabase.co

# Supabase service role key (server-only secret) - keep this out of client code
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Port for the backend server (optional)
PORT=3000
